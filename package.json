{
  "name": "first_nodejs",
  "version": "1.0.0",
  "description": "learn nodejs",
  "main": "dist/app.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "nodemon ",
    "dev": "nodemon",
    "start:debug": "nodemon --inspect=9229 -e ts,tsx --exec node -r ts-node/register -r tsconfig-paths/register ./src/app.ts"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts",
    "ignore": [
      "node_modules"
    ],
    "exec": "ts-node -r tsconfig-paths/register ./src/app.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/trancuong21102/learn-nodejs.git"
  },
  "keywords": [
    "nodejs",
    "express"
  ],
  "author": "pinnih",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/trancuong21102/learn-nodejs/issues"
  },
  "homepage": "https://github.com/trancuong21102/learn-nodejs#readme",
  "dependencies": {
    "dotenv": "16.4.7",
    "ejs": "3.1.10",
    "express": "5.0.1",
    "mysql2": "^3.18.0"
  },
  "devDependencies": {
    "@types/ejs": "3.1.5",
    "@types/express": "5.0.0",
    "@types/node": "^22.10.7",
    "nodemon": "3.1.9",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.7.3"
  }
}